FROM debian:latest
FROM python:3

LABEL MAINTAINER="https://gitlab.com/KasRoudra/UltimatePhishing-python"

WORKDIR /UltimatePhishing-python/
ADD . /UltimatePhishing-python

RUN apt-get update
RUN apt-get upgrade -y
RUN apt-get install python3-pip php openssh-client -y
RUN pip3 install -r files/requirements.txt --break-system-packages
RUN apt-get clean

CMD ["python3", "UltimatePhishing-python.py", "--noupdate"]
